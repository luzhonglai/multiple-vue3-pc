:export {
  menuText: #ccc;
  menuActiveText: #fff;
  menuActiveBg: #5587F0;
  menuBg: #252A2F;
  subMenuBg: #000000;
  subMenuHover: #5587F0;
  menuWidth: 200px;
  menuMinWidth: 60px;
}
.app__wrap {
  position: relative;
  height: 100%;
  width: 100%;
}
.app__wrap .sidebar__wrap {
  position: fixed;
  width: 200px;
  top: 0;
  left: 0;
  height: 100%;
  transition: width 0.2s;
}
.app__wrap .sidebar__wrap--collapsed {
  width: 60px;
}
.app__wrap .sidebar__wrap--collapsed :deep(.anticon-item) {
  display: none;
}
.app__wrap .main__wrap {
  position: absolute;
  width: calc(100% - 200px);
  height: 100%;
  top: 0;
  left: 200px;
  transition: all 0.2s;
  z-index: 1;
}
.app__wrap .main__wrap .header__wrap {
  transition: all 0.2s;
}
.app__wrap .main__wrap .header__wrap .navbar__wrap {
  display: flex;
  align-items: center;
  height: 40px;
  padding: 0 20px 0 15px;
  position: relative;
  background: #fff;
}
.app__wrap .main__wrap .header__wrap .navbar__wrap:after {
  content: "";
  width: 100%;
  height: 1px;
  border-top: 1px solid #d8dce5;
  position: absolute;
  bottom: 0;
  left: 0;
}
.app__wrap .main__wrap .header__wrap .navbar__wrap :deep(.hover-container) {
  transition: background 0.2s;
  height: 100%;
  line-height: 45px;
  padding: 0 5px;
  text-align: center;
}
.app__wrap .main__wrap .header__wrap .navbar__wrap :deep(.hover-container):hover {
  background: #f6f6f6;
}
.app__wrap .main__wrap .header__wrap .navbar__wrap .navbar__wrap--right {
  display: flex;
  align-items: center;
  height: 40px;
  position: absolute;
  top: 0;
  right: 20px;
}
.app__wrap .main__wrap .header__wrap .navbar__wrap .navbar__wrap--right :deep(.screenfull-container),
.app__wrap .main__wrap .header__wrap .navbar__wrap .navbar__wrap--right :deep(.user-container) {
  line-height: 40px !important;
}
.app__wrap .main__wrap .main__wrap--content {
  height: 100%;
}
.app__wrap .main__wrap .main__wrap--content :deep(.el-scrollbar__wrap) {
  overflow-x: hidden;
}
.app__wrap .main__wrap .main__wrap--content :deep(.el-scrollbar__view) {
  height: 100%;
}
.app__wrap .main__wrap--collapsed {
  width: calc(100% - 60px);
  left: 60px;
}
.app__wrap--LeftTop .main__wrap--fixed--all,
.app__wrap--Classic .main__wrap--fixed--all {
  margin-top: 80px !important;
  height: calc(100% - 40px - 40px) !important;
}
.app__wrap--LeftTop .main__wrap--fixed--nav,
.app__wrap--Classic .main__wrap--fixed--nav {
  margin-top: 40px !important;
  height: calc(100% - 40px) !important;
}
.app__wrap--LeftTop .main__wrap--fixed--tags,
.app__wrap--Classic .main__wrap--fixed--tags {
  margin-top: 40px !important;
  height: calc(100% - 40px) !important;
}
.app__wrap--LeftTop .header__wrap--fixed,
.app__wrap--Classic .header__wrap--fixed {
  position: fixed !important;
  width: calc(100% - 200px) !important;
  top: 60px !important;
  left: 200px !important;
  z-index: 200;
}
.app__wrap--LeftTop .header__wrap--collapsed,
.app__wrap--Classic .header__wrap--collapsed {
  width: calc(100% - 60px) !important;
  left: 60px !important;
}
.app__wrap--Classic .header__wrap--fixed {
  top: 0 !important;
}
.app__wrap--LeftTop .sidebar__wrap--Top,
.app__wrap--Top .sidebar__wrap--Top {
  height: 60px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 0 20px;
  background-color: #fff;
  position: relative;
}
.app__wrap--LeftTop .sidebar__wrap--Top .sidebar__item--Top,
.app__wrap--Top .sidebar__wrap--Top .sidebar__item--Top {
  flex: 1;
  margin: 0 50px;
}
.app__wrap--LeftTop .sidebar__wrap--Top .navbar__wrap--right,
.app__wrap--Top .sidebar__wrap--Top .navbar__wrap--right {
  display: flex;
  align-items: center;
  height: 60px;
}
.app__wrap--LeftTop .sidebar__wrap--Top .navbar__wrap--right :deep(.hover-container),
.app__wrap--Top .sidebar__wrap--Top .navbar__wrap--right :deep(.hover-container) {
  transition: background 0.2s;
  height: 100%;
  line-height: 60px;
  padding: 0 5px;
  text-align: center;
}
.app__wrap--LeftTop .sidebar__wrap--Top .navbar__wrap--right :deep(.hover-container):hover,
.app__wrap--Top .sidebar__wrap--Top .navbar__wrap--right :deep(.hover-container):hover {
  background: #f6f6f6;
}
.app__wrap--Top .header__wrap--fixed {
  position: fixed !important;
  width: 100% !important;
  top: 60px !important;
  left: 0 !important;
  z-index: 200;
}
.app__wrap--Top .main__wrap {
  width: 100%;
  left: 0;
  height: calc(100% - 60px);
  top: 60px;
}
.app__wrap--Top .main__wrap--fixed--all,
.app__wrap--Top .main__wrap--fixed--tags {
  margin-top: 40px !important;
  height: calc(100% - 40px) !important;
}
.app__wrap--LeftTop .sidebar__wrap {
  top: 60px;
  left: 0;
  height: calc(100% - 60px);
}
.app__wrap--LeftTop .main__wrap {
  width: calc(100% - 200px);
  left: 200px;
  height: calc(100% - 60px);
  top: 60px;
}
.app__wrap--LeftTop .main__wrap--collapsed {
  width: calc(100% - 60px);
  left: 60px;
}
